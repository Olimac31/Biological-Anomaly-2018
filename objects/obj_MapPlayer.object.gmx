intMoveY = keyUp + keyDown;
}
else if reading = 1
{
intMoveX = 0;
intMoveY = 0;
}
//Move X and Y axis
repeat(moveVel)
{
    if(!place_meeting(x + sign(intMoveX), y, obj_block01))
